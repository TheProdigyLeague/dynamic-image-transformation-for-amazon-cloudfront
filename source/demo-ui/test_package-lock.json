{
    "tests": [
        {
            "description": "Root package metadata is correct",
            "assert": {
                "name": "demo-ui",
                "version": "7.0.3",
                "lockfileVersion": 3,
                "requires": true
            }
        },
        {
            "description": "Root package dependencies are correct",
            "assert": {
                "packages..dependencies": {
                    "@popperjs/core": "^2.11.8",
                    "bootstrap": "^5.3.3",
                    "jquery": "^3.7.1"
                }
            }
        },
        {
            "description": "@popperjs/core package is present with correct version and integrity",
            "assert": {
                "packages.node_modules/@popperjs/core.version": "2.11.8",
                "packages.node_modules/@popperjs/core.integrity": "sha512-P1st0aksCrn9sGZhp8GMYwBnQsbvAWsZAX44oXNNvLHGqAOcoVxmjZiohstwQ7SqKnbR47akdNi+uleWD8+g6A=="
            }
        },
        {
            "description": "bootstrap package is present with correct version, integrity, and peerDependencies",
            "assert": {
                "packages.node_modules/bootstrap.version": "5.3.3",
                "packages.node_modules/bootstrap.integrity": "sha512-8HLCdWgyoMguSO9o+aH+iuZ+aht+mzW0u3HIMzVu7Srrpv7EBBxTnrFlSCskwdY1+EOFQSm7uMJhNQHkdPcmjg==",
                "packages.node_modules/bootstrap.peerDependencies": {
                    "@popperjs/core": "^2.11.8"
                }
            }
        },
        {
            "description": "jquery package is present with correct version and integrity",
            "assert": {
                "packages.node_modules/jquery.version": "3.7.1",
                "packages.node_modules/jquery.integrity": "sha512-m4avr8yL8kmFN8psrbFFFmB/If14iN5o9nw/NgnnM+kybDJpRsAynV2BsfpTYrTRysYUdADVD7CkUUizgkpLfg=="
            }
        }
    ]
}